public class Fase3 {
	public static void main(String[]args) {
		final int year1 = 1948;
		final int myBornYear = 1975;
		boolean isOrNot = false;
		for (int i = year1; i<=myBornYear; i++) {
			if (i % 4 == 0) {
				isOrNot = true;
				if (isOrNot==true) {
					System.out.println("El año "+i+" es bisiesto.");
				}
				else {
					isOrNot = false;
				}
			}
		}
	}
}